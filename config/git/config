[init]
    defaultBranch = main
[core]
	fileMode = false
	precomposeunicode = false
	pager = delta 
[interactive]
	diffFilter = delta --color-only
[push]
	default = simple
[branch]
    autoSetupMerge = simple
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
	process = git-lfs filter-process
[delta]
    navigate = true
    syntax-theme = GitHub
    line-numbers = true
[merge]
    conflictstyle = diff3
[diff]
    colorMoved = default
[pull]
	rebase = true
[rebase]
    autoStash = true
